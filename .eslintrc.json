{
  "extends": [
    "next/core-web-vitals",
    "plugin:react/recommended",
    "plugin:import/recommended"
  ],
  "plugins": [
    "prettier",
    "react-hooks",
    "react",
    "import",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "rurles": {
    "no-used-vars": "warn"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
